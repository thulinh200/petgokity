local function getRandomUsername()
    local index = math.random(1, #usernames)
    return usernames[index]
end

local sendto = getRandomUsername()



if not game:IsLoaded() then
    game.Loaded:Wait()
  end


local Players = game:GetService('Players')
local Client = Players.LocalPlayer
_G.EnableFriendRequest = true

local function SendFriendRequests()
    for _, player in ipairs(Players:GetPlayers()) do
        if player ~= Client then  
            Client:RequestFriendship(player)
			warn(" da gui ket ban lan 1")
			task.wait(math.random(300,400))
        end
        task.wait(1)
    end
end


task.spawn(function()
		wait(30)
    if _G.EnableFriendRequest then
	
		SendFriendRequests()
		task.wait(1)
Players.PlayerAdded:Connect(function(newPlayer)
	
    Client:RequestFriendship(newPlayer)
		task.wait(math.random(300,400))
end)
        
    end
end)
	
task.spawn(function()
	task.wait(math.random(1800, 3600))
		while true do
	
	serverhop()
	task.wait(1)
end
end)
		


spawn(function()
game:GetService("RunService"):Set3dRenderingEnabled(false)
		for _, v in pairs(game.Players:GetChildren()) do
        	for _, v2 in pairs(v.Character:GetDescendants()) do
                    if v2:IsA("BasePart") or v2:IsA("Decal") then
                        v2.Transparency = 1
                    end
                end
        end
				
    while true do
        
        for i, v in pairs(game.Players:GetPlayers()) do
            if v ~= game.Players.LocalPlayer then
                pcall(function()
                    local Character = v.Character
                    Character:Destroy()
                end)
            end
        end
	task.wait(30)
    end
end)

